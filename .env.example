# =============================================================================
# LangChain Question Extraction Agent - Environment Configuration
# =============================================================================
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control
# =============================================================================

# -----------------------------------------------------------------------------
# Vision Model Configuration (Doubao)
# -----------------------------------------------------------------------------
# API key for Doubao vision model (required for image analysis)
DOUBAO_API_KEY=your_doubao_api_key_here

# Doubao API base URL
DOUBAO_BASE_URL=https://ark.cn-beijing.volces.com/api/v3

# Doubao model endpoint ID
DOUBAO_MODEL=doubao-seed-1-6-251015

# Maximum tokens for vision model response
DOUBAO_MAX_TOKENS=4096

# Temperature for vision model (lower = more deterministic)
DOUBAO_TEMPERATURE=0.1

# -----------------------------------------------------------------------------
# Agent LLM Configuration (OpenAI-compatible)
# -----------------------------------------------------------------------------
# Agent LLM Configuration
# -----------------------------------------------------------------------------
# Provider for agent LLM (openai, anthropic)
AGENT_PROVIDER=openai

# API key for Anthropic (required if AGENT_PROVIDER=anthropic)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# API key for agent LLM (optional - defaults to DOUBAO_API_KEY if not set)
# AGENT_API_KEY=your_agent_api_key_here

# Model to use for agent reasoning (defaults to doubao-seed-1-6-251015)
AGENT_MODEL=doubao-seed-1-6-flash-250828

# Base URL for agent LLM API (defaults to Doubao API URL)
AGENT_BASE_URL=https://ark.cn-beijing.volces.com/api/v3

# Maximum iterations for agent execution
AGENT_MAX_ITERATIONS=10

# Enable verbose logging for agent
AGENT_VERBOSE=true

# Enable conversation memory
AGENT_ENABLE_MEMORY=true

# Temperature for agent LLM (0 = deterministic)
AGENT_TEMPERATURE=0.0

# -----------------------------------------------------------------------------
# LangSmith Configuration
# -----------------------------------------------------------------------------
# Enable LangSmith tracing
LANGCHAIN_TRACING_V2=true

# LangSmith API key
LANGCHAIN_API_KEY=your_langsmith_api_key_here

# LangSmith project name
LANGCHAIN_PROJECT=image2test

# LangSmith API endpoint (optional)
# LANGCHAIN_ENDPOINT=https://api.smith.langchain.com

# -----------------------------------------------------------------------------
# Output Configuration
# -----------------------------------------------------------------------------
# Default directory for output files
DEFAULT_OUTPUT_DIR=./output

# Default filename for JSON output
DEFAULT_JSON_FILENAME=questions.json

# Default filename for Word output
DEFAULT_WORD_FILENAME=questions.docx

# Automatically save extracted questions
AUTO_SAVE=true

# Append to existing files by default (false = overwrite)
APPEND_BY_DEFAULT=false

# JSON indentation spaces
JSON_INDENT=2

# -----------------------------------------------------------------------------
# Logging
# -----------------------------------------------------------------------------
# Log level: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO
